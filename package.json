{
  "name": "@workablehr/oneflow",
  "version": "1.4.1",
  "description": "OneFlow cli tool",
  "main": "build/lib/index.js",
  "repository": "Workable/oneflow",
  "scripts": {
    "clean": "rm -rf ./build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "prewatch": "npm run clean",
    "watch": "tsc -w -p .",
    "prepublishOnly": "npm test",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "pretest": "npm run lint && npm run build",
    "test": "istanbul test _mocha build/test/index build/test -- --recursive -R spec --timeout 10000",
    "posttest": "remap-istanbul -i coverage/coverage.json -o coverage/lcov-report -t html"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "bin": {
    "oneflow": "bin/oneflow"
  },
  "author": "Nikos Kostoulas <nikostoulas@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/chalk": "^0.4.31",
    "@types/inquirer": "0.0.35",
    "@types/mocha": "^2.2.41",
    "@types/node": "^9.4.6",
    "@types/should": "^8.3.0",
    "@types/source-map-support": "^0.4.0",
    "chalk": "^1.1.3",
    "commander": "^2.11.0",
    "commander-tabtab": "^0.1.0",
    "figlet": "1.2.0",
    "home-config": "^0.1.0",
    "inquirer": "3.0.5",
    "moment": "^2.22.2",
    "semver": "^5.3.0",
    "tsconfig-paths": "^2.3.0",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "remap-istanbul": "^0.9.5",
    "should": "^11.2.1",
    "source-map-support": "^0.4.15",
    "tslint": "^5.4.3"
  }
}
